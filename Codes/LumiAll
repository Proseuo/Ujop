	timeout /t 5 /nobreak >nul
	echo.
	cls
	goto WinDefRemover
)



echo.
echo WinDefRemover Started..
timeout /t 20 /nobreak >nul
echo Done.
timeout /t 5 /nobreak >nul
cls
goto start
















